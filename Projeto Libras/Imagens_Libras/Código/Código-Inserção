if image1.Height = 105 then
      begin
        image1.height := 95;
        image1.Width := 95;
        image1.left := image1.left + 5;
        image1.top := image1.top + 5;

        str := str + 'C';
        btVerificar.Click;

      end
      else
        begin
           image1.height := 105;
          image1.Width := 105;
          image1.left := image1.left - 5;
          image1.top := image1.top - 5;
        end;



